{
  "name": "@times-components-native/android-app",
  "private": true,
  "version": "1.0.2",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "./gradlew clean && ./gradlew generateReactArchives && ./gradlew assemble && ./gradlew publish",
