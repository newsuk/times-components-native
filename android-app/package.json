  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-svg": "9.11.1",
    "react-native-v8": "0.61.5-patch.3",
    "react-native-webview": "7.0.5",
    "url-polyfill": "1.1.0",
    "v8-android": "8.80.1"
  }
}
