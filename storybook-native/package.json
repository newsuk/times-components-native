{
  "name": "@times-components-native/storybook-native",
  "private": true,
  "version": "0.2.13",
  "license": "BSD-3-Clause",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "postinstall": "rnstl",
    "start": "react-native start --config metro.config.js"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "../packages"
      ],
      "pattern": "!(brightcove-video)/*.stories!(.web).js",
      "outputFile": "./story-loader.js"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.11.0",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-info": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-ondevice-knobs": "5.3.18",
    "@storybook/addon-options": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@storybook/react-native": "5.3.18",

    "core-js": "^2.6.5",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-svg": "9.11.1",
    "regenerator-runtime": "0.13.2",
    "url-polyfill": "1.1.0"
  }
}
