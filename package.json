{
  "name": "times-components-native",
  "private": true,
  "version": "4.0.0",
  "description": "A collection of react-native components for The Times and Sunday Times",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0 <13",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "start": "rnstl && react-native start --config ./app/metro.config.js",
    "depcheck": "depcheck --config=./.depcheckrc",
    "lint": "eslint . && yarn depcheck",
    "ios:native": "react-native start --config ./lib/ios/metro.config.js",
    "ios:build-lib": "yarn ios:build-lib:js && yarn ios:build-lib:update-version",
    "ios:build-lib:js": "mkdir -p lib/ios/assets/js && mkdir -p lib/ios/assets/res/graphql && cp ./packages/provider-queries/src/*.graphql ./lib/ios/assets/res/graphql && react-native ram-bundle --platform ios --dev false --entry-file lib/ios/index.ios.js --bundle-output lib/ios/assets/js/index.ios.bundle --assets-dest lib/ios/assets/res/",
    "ios:build-lib:update-version": "cat package.json | grep version | head -1 | sed 's/[\",\t ]//g' | awk -F: '{ print \"Bundle Version: \" $2 }' > lib/ios/assets/js/version.meta",
    "android:native": "react-native start --config ./lib/android/metro.config.js",
    "android:build-lib": "yarn android:build-lib:js && cp -r node_modules/@react-native-community/netinfo node_modules/react-native-community-netinfo && cp -r node_modules/@react-native-community/art node_modules/react-native-community-art && yarn android:build-lib:aar",
    "android:build-lib:js": "mkdir -p lib/android/xnative/src/main/assets/ && mkdir -p lib/android/xnative/src/main/res/ && react-native ram-bundle --platform android --dev false --minify --entry-file lib/android/index.android.js --bundle-output lib/android/xnative/src/main/assets/index.android.bundle --assets-dest lib/android/xnative/src/main/res/",
    "android:build-lib:aar": "cd lib/android && ./gradlew clean && ./gradlew generateReactArchives && ./gradlew assemble && ./gradlew publish && cd -",
    "typecheck": "tsc --project ./",
    "postinstall": "yarn gen:types && node ./scripts/fetch-fonts",
    "gen:types": "node packages/schema/fetchSchema.js && gql-gen",
    "test:ios": "jest --config test-setup/jest.ios.config.js",
    "test:android": "jest --config test-setup/jest.android.config.js",
    "test:common": "jest --config test-setup/jest.common.config.js",
    "test:all": "yarn test:ios && yarn test:android && yarn test:common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "times",
    "components",
    "newsuk"
  ],
  "author": "News UK & Ireland Ltd",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components-native/issues"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "packages"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./app/storybook/story-loader.js"
    }
  },
  "homepage": "https://github.com/newsuk/times-components-native#readme",
  "dependencies": {
    "@react-native-community/art": "1.2.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/netinfo": "5.9.9",
    "@skele/components": "1.0.0-alpha.40",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.4",
    "apollo-link-http": "1.5.14",
    "apollo-utilities": "1.2.1",
    "css": "^2.2.1",
    "date-fns": "1.28.5",
    "graphql": "14.2.1",
    "graphql-tools": "4.0.4",
    "hoist-non-react-statics": "2.5.5",
    "hyphenate-style-name": "1.0.2",
    "hyphenation.en-us": "^0.2.1",
    "hypher": "^0.2.5",
    "jest-plugin-context": "2.9.0",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.4.0",
    "lodash.merge": "4.6.2",
    "lodash.omit": "4.4.0",
    "lodash.omitby": "4.6.0",
    "lodash.pick": "4.4.0",
    "luxon": "1.18.0",
    "memoize-one": "5.1.1",
    "mockdate": "2.0.2",
    "node-fetch": "^2.6.0",
    "opentype.js": "1.1.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-apollo": "2.5.5",
    "react-display-name": "0.2.3",
    "react-dom": "16.9.0",
    "react-native": "0.63.4",
    "react-native-autoheight-webview": "1.5.1",
    "react-native-device-info": "7.3.1",
    "react-native-image-zoom-viewer": "3.0.1",
    "react-native-navigation": "^6.11.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-svg": "12.1.0",
    "react-native-v8": "0.63.4-patch.1",
    "react-native-webview": "10.9.3",
    "unfetch": "^4.1.0",
    "url-parse": "1.4.7",
    "v8-android": "9.88.0",
    "zen-observable": "0.8.11"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "7.10.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/runtime": "^7.8.4",
    "@bam.tech/react-native-graphql-transformer": "0.1.5",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-ondevice-knobs": "5.3.19",
    "@storybook/react-native": "5.3.19",
    "@types/jest": "24.0.2",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.omitby": "^4.6.6",
    "@types/react-native": "0.63.6",
    "@types/react-test-renderer": "16.9.3",
    "@types/url-parse": "1.4.3",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "conventional-github-releaser": "^3.1.5",
    "depcheck": "1.0.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-native": "^3.8.1",
    "fetch-mock": "8.3.1",
    "global": "4.3.2",
    "graphql-code-generator": "0.14.5",
    "graphql-codegen-typescript-template": "0.14.5",
    "graphql-tag": "2.10.3",
    "jest": "24.8.0",
    "jest-transform-graphql": "2.1.0",
    "jsdom": "9.12.0",
    "marked": "^1.1.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "2.0.5",
    "react-native-storybook-loader": "2.0.2",
    "react-test-renderer": "16.13.1",
    "regenerator-runtime": "^0.13.7",
    "typescript": "3.7.2",
    "url": "0.11.0"
  }
}
