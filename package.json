{
  "name": "times-components-native",
  "private": true,
  "version": "0.0.1",
  "description": "A collection of react-native components for The Times and Sunday Times",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0 <13",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "test:all": "lerna run test --since --stream && lerna run test:all --since --stream",
    "prelint": "eslint . && prettier --list-different '**/*.js' && yarn deps:lint",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,@thetimes,identity-obj-proxy,graphql,jest,prettier,react-art,graphql-codegen-typescript-template,@types/jest,graphql-code-generator,ts-jest,typescript,typings' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "deps:lint": "lerna run cli -- -- --hint --bail -s majorityProgressive",
    "deps:fix": "lerna run cli -- -- --fix -s majorityProgressive",
    "deps:list": "lerna run cli -- -- --list -s majorityProgressive",
    "fetch-fonts": "node ./lib/fetch-fonts",
    "prestorybook": "yarn storybook:build-vendor",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./dist/public",
    "storybook:build-vendor": "webpack --config ./.storybook/vendor.webpack.config.js",
    "storybook:build": "yarn storybook:build-vendor && build-storybook -c .storybook -s ./dist/public",
    "prestorybook:test-build": "yarn storybook:build-vendor",
    "storybook:test-build": "build-storybook -c .storybook -s ./dist/public",
    "storybook-native": "react-native start --config ./storybook-native/metro.config.js",
    "ios": "react-native run-ios --no-packager",
    "ios:logs": "yarn ios && react-native log-ios",
    "ios:app": "react-native start --config ./ios-app/metro.config.js",
    "ios:build-lib": "yarn ios:build-lib:js && cd ios-app && yarn build",
    "ios:build-lib:js": "mkdir -p ios-app/ios-assets/js && mkdir -p ios-app/ios-assets/res/graphql && cp ./packages/provider-queries/src/*.graphql ./ios-app/ios-assets/res/graphql && react-native ram-bundle --platform ios --dev false --entry-file ios-app/index.ios.js --bundle-output ios-app/ios-assets/js/index.ios.bundle --assets-dest ios-app/ios-assets/res/",
    "ios:publish-lib": "./lib/publish-ios-assets.sh -v",
    "android:app": "react-native start --config ./android-app/metro.config.js",
    "android:build-lib": "yarn android:build-lib:js && cp -r node_modules/@react-native-community/netinfo node_modules/react-native-community-netinfo && cd android-app && yarn build",
    "android:build-lib:js": "mkdir -p android-app/xnative/src/main/assets/ && mkdir -p android-app/xnative/src/main/res/ && react-native ram-bundle --platform android --dev false --minify --entry-file android-app/index.android.js --bundle-output android-app/xnative/src/main/assets/index.android.bundle --assets-dest android-app/xnative/src/main/res/",
    "android:publish-lib": "lerna run --scope @times-components-native/android-app publish-library --stream",
    "prefmt": "prettier --write '**/*.{js,json}'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "postinstall": "yarn run fetch-fonts",
    "test:ios": "lerna run test:ios --stream",
    "test:android": "lerna run test:android --stream",
    "test:unit": "lerna run test:unit --stream"
  },
  "jest": {
    "preset": "react-native",
    "coveragePathIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-native-svg)",
      "packages/?!author-profile"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web"
  ],
  "author": "News UK & Ireland Ltd",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.10.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@bam.tech/react-native-graphql-transformer": "0.1.5",
    "@bitjson/npm-scripts-info": "1.0.0",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@simple-dom/interface": "1.4.0",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-info": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-options": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@storybook/react-native": "5.3.18",
    "@thetimes/jest-lint": "*",
    "@types/jest": "24.0.2",
    "@types/node": "12.7.12",
    "@types/point-in-polygon": "1.0.0",
    "@types/react-native": "0.60.21",
    "@types/react-test-renderer": "16.9.0",
    "@types/url-parse": "1.4.3",
    "apollo-client": "2.5.1",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "codecov": "3.6.1",
    "cz-conventional-changelog": "2.1.0",
    "dashify": "0.2.2",
    "depcheck": "0.6.9",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "es6-promise-pool": "^2.5.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "2.0.1",
    "fetch-mock": "8.3.1",
    "fs-extra": "7.0.0",
    "gh-pages": "2.0.1",
    "global": "4.3.2",
    "graphql": "14.2.1",
    "graphql-code-generator": "0.14.5",
    "graphql-codegen-typescript-template": "0.14.5",
    "graphql-tag": "2.10.3",
    "handlebars": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-transform-graphql": "2.1.0",
    "jsdom": "9.12.0",
    "lodash.merge": "4.6.2",
    "lodash.omit": "4.4.0",
    "metro-react-native-babel-preset": "0.53.1",
    "mkdirp": "0.5.1",
    "node-fetch": "2.2.0",
    "npm-run-all": "4.1.5",
    "open-cli": "5.0.0",
    "opentype.js": "1.1.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-art": "16.6.3",
    "react-dom": "16.9.0",
    "react-native": "0.61.5",
    "react-native-storybook-loader": "1.8.0",
    "react-test-renderer": "16.9.0",
    "rimraf": "2.6.1",
    "sort-pkg": "1.1.0",
    "subcommander": "1.0.0",
    "trash-cli": "3.0.0",
    "ts-jest": "23.10.4",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "typescript": "3.7.2",
    "typings": "^2.1.1",
    "url": "0.11.0",
    "url-polyfill": "1.1.0",
    "watch": "1.0.2",
    "webpack": "4.30.0"
  },
  "dependencies": {
    "@react-native-community/netinfo": "5.3.3",
    "@skele/components": "1.0.0-alpha.40",
    "lodash.pick": "4.4.0",
    "mockdate": "2.0.2",
    "prop-types": "15.7.2",
    "react-broadcast": "0.5.2",
    "react-native-device-info": "5.3.1",
    "react-native-webview": "7.0.5"
  }
}
