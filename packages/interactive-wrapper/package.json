{
  "name": "@times-components-native/interactive-wrapper",
  "private": true,
  "version": "0.8.2",
  "description": "Wrapper for legacy Interactive components",
  "main": "src/interactive-wrapper",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,identity-obj-proxy,jest,prettier' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "interactive",
    "interactive-wrapper",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components-native/eslint-config-thetimes": "0.8.16",
    "@times-components-native/jest-configurator": "2.6.11",
    "@times-components-native/jest-serializer": "3.2.23",
    "@times-components-native/storybook": "4.1.54",
    "babel-jest": "24.8.0",
    "enzyme": "3.9.0",
    "eslint": "5.9.0",
    "fetch-mock": "8.3.1",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "0.61.5",
    "react-test-renderer": "16.9.0"
  },
  "dependencies": {
    "@times-components-native/utils": "6.3.14",
    "prop-types": "15.7.2",
    "react-native-webview": "7.0.5"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
