{
  "name": "@times-components-native/interactive-wrapper",
  "private": true,
  "version": "0.8.2",
  "description": "Wrapper for legacy Interactive components",
  "main": "src/interactive-wrapper",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,identity-obj-proxy,jest,prettier' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "interactive",
    "interactive-wrapper",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/jest-configurator": "file:../jest-configurator",
    "@times-components-native/jest-serializer": "file:../jest-serializer",
    "@times-components-native/storybook": "file:../storybook"
  },
  "dependencies": {
    "@times-components-native/utils": "file:../utils",
    "prop-types": "15.7.2",
    "react-native-autoheight-webview": "1.5.1",
    "react-native-webview": "7.0.5"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
