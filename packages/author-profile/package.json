{
  "name": "@times-components-native/author-profile",
  "private": true,
  "version": "6.3.14",
  "description": "Author profile information along with a list of articles they have written",
  "main": "src/author-profile",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,react-art' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "author-profile",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/eslint-config-thetimes": "file:../eslint-config-thetimes",
    "@times-components-native/jest-configurator": "file:../jest-configurator",
    "@times-components-native/jest-serializer": "file:../jest-serializer",
    "@times-components-native/mocks": "file:../mocks",
    "@times-components-native/provider-queries": "file:../provider-queries",
    "@times-components-native/provider-test-tools": "file:../provider-test-tools",
    "@times-components-native/storybook": "file:../storybook",
    "@times-components-native/tealium-utils": "file:../tealium-utils",
    "@times-components-native/test-utils": "file:../test-utils"
  },
  "dependencies": {
    "@times-components-native/article-list": "file:../article-list",
    "@times-components-native/gradient": "file:../gradient",
    "@times-components-native/icons": "file:../icons",
    "@times-components-native/image": "file:../image",
    "@times-components-native/link": "file:../link",
    "@times-components-native/markup": "file:../markup",
    "@times-components-native/markup-forest": "file:../markup-forest",
    "@times-components-native/pagination": "file:../pagination",
    "@times-components-native/provider": "file:../provider",
    "@times-components-native/responsive": "file:../responsive",
    "@times-components-native/styleguide": "file:../styleguide",
    "@times-components-native/tracking": "file:../tracking",
    "@times-components-native/utils": "file:../utils",
    "lodash.get": "4.4.2",
    "prop-types": "15.7.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
