{
  "name": "@times-components-native/video",
  "private": true,
  "version": "4.10.18",
  "main": "src/video",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,react-art' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:all": "yarn test:ios --coverage && yarn test:android --coverage"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../..",
    "coverageDirectory": "<rootDir>/packages/video/coverage/",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/react-native"
    ],
    "testMatch": [
      "<rootDir>/packages/video/__tests__/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "video",
    "brightcove",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components-native/icons": "file:../icons",
    "@times-components-native/image": "file:../image",
    "@times-components-native/styleguide": "file:../styleguide",
    "@times-components-native/svgs": "file:../svgs",
    "prop-types": "15.7.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "_timesComponentsCliVariables": {
    "component": "Video",
    "packageDescription": "Video component",
    "packageName": "video"
  },
  "publishConfig": {
    "access": "public"
  }
}
