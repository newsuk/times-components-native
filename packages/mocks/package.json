{
  "name": "@times-components-native/mocks",
  "private": true,
  "version": "0.1.15",
  "description": "Shared mocks for the tests",
  "main": "src/index",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier*,react-native' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "mocks",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components-native/styleguide": "file:../styleguide"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
