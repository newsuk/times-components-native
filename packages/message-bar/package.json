{
  "name": "@times-components-native/message-bar",
  "private": true,
  "version": "0.3.52",
  "description": "A component for showing user notifications",
  "main": "src/index",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,@thetimes/jest-lint,jest,prettier' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.{js,json}'",
    "prettier:diff": "prettier --list-different '**/*.{js,json}'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "message-bar",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/storybook": "file:../storybook",
    "@times-components-native/test-utils": "file:../test-utils"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "dependencies": {
    "@times-components-native/icons": "file:../icons",
    "@times-components-native/responsive": "file:../responsive",
    "@times-components-native/styleguide": "file:../styleguide",
    "prop-types": "15.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
