{
  "name": "@times-components-native/lazy-load",
  "private": true,
  "version": "0.6.13",
  "description": "Use IntersectionObserver to lazy load resources",
  "main": "dist/lazy-load",
  "dev": "src/lazy-load",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,webpack*' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "prepublishOnly": "yarn transpile && yarn bundle",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react-native-web",
    "react",
    "native",
    "web",
    "lazy-load",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components-native/eslint-config-thetimes": "0.8.16",
    "@times-components-native/storybook": "4.1.54",
    "babel-jest": "24.8.0",
    "eslint": "5.9.0",
    "jest": "24.8.0",
    "prettier": "1.14.3",
    "react": "16.9.0",
    "styled-components": "4.3.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  },
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59",
    "react-native-web": "0.11.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
