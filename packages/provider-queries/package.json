{
  "name": "@times-components-native/provider-queries",
  "private": true,
  "version": "2.14.1",
  "description": "GraphQL queries for sharing",
  "main": "src/provider-queries",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "cpgql": "find . -name \"*.graphql\" -exec cp {} ./dist/ \\;",
    "cleanup-graphql": "rm -rf ./node_modules/.cache/babel-loader"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "GraphQL"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/eslint-config-thetimes": "0.8.16",
    "babel-plugin-import-graphql": "2.7.0",
    "eslint-plugin-graphql": "2.1.1",
    "prettier": "1.14.3"
  },
  "dependencies": {
    "apollo-utilities": "1.2.1",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
