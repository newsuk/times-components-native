{
  "name": "@times-components-native/section",
  "private": true,
  "version": "1.6.67",
  "description": "Section Page",
  "main": "src/section",
  "scripts": {
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier' --ignore-bin-package=false --skip-missing",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:unit": "jest --config='./__tests__/unit/jest.config.js' ./__tests__/unit",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test:unit --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "section",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "dependencies": {
    "@times-components-native/context": "file:../context",
    "@times-components-native/edition-slices": "file:../edition-slices",
    "@times-components-native/icons": "file:../icons",
    "@times-components-native/image": "file:../image",
    "@times-components-native/link": "file:../link",
    "@times-components-native/responsive": "file:../responsive",
    "@times-components-native/styleguide": "file:../styleguide",
    "@times-components-native/tracking": "file:../tracking",
    "lodash.get": "4.4.2",
    "luxon": "1.18.0",
    "memoize-one": "5.1.1",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@times-components-native/star-button": "file:../star-button",
    "@times-components-native/storybook": "file:../storybook",
    "@times-components-native/tealium-utils": "file:../tealium-utils"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
