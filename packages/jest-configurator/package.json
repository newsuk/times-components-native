{
  "name": "@times-components-native/jest-configurator",
  "private": true,
  "version": "2.6.11",
  "description": "Cross platform Jest Config",
  "main": "src/jest-configurator.js",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,identity-obj-proxy,jest*,prettier' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "pretest": "./setup.sh",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "link",
    "component",
    "jest",
    "eslint"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "rootDir": "../../",
    "collectCoverageFrom": [
      "**/jest-configurator/src/**"
    ],
    "coverageDirectory": "<rootDir>/packages/jest-configurator/coverage",
    "coveragePathIgnorePatterns": [
      "react-native-mock-components.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/react-native"
    ],
    "testMatch": [
      "<rootDir>/packages/jest-configurator/__tests__/*.test.js"
    ]
  },
  "dependencies": {
    "@babel/core": "7.4.4",

    "babel-jest": "24.8.0",
    "babel-plugin-istanbul": "5.1.3",
    "babel-preset-jest": "24.6.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "find-node-modules": "1.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-plugin-context": "2.9.0",
    "jest-plugins": "2.9.0"
  },
  "resolutions": {
    "minimatch": "3.0.4"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  }
}
