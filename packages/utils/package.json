{
  "name": "@times-components-native/utils",
  "private": true,
  "version": "6.3.14",
  "description": "A set of helpers and/or workarounds to be shared across packages",
  "main": "src/index",
  "scripts": {
    "prettier:diff": "prettier --list-different '**/*.*'",
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test": "jest --config='./__tests__/jest.config.js'",
    "transpile": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "utils",
    "graphql"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@times-components-native/schema": "file:../schema",
    "@times-components-native/styleguide": "file:../styleguide",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "lodash.omitby": "4.6.0",
    "prop-types": "15.7.2",
    "react-native": "0.61.5",
    "styled-components": "4.3.2",
    "unfetch": "^3.0.0"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  }
}
