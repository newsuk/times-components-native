    "@times-components-native/card": "file:../card",
    "@times-components-native/error-view": "file:../error-view",
    "@times-components-native/link": "file:../link",
    "@times-components-native/responsive": "file:../responsive",
    "@times-components-native/styleguide": "file:../styleguide",
    "@times-components-native/tracking": "file:../tracking",
    "@times-components-native/utils": "file:../utils",
    "@times-components-native/watermark": "file:../watermark",
    "lodash.get": "4.4.2",
