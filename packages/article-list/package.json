    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "article-list",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/eslint-config-thetimes": "file:../eslint-config-thetimes",
    "@times-components-native/jest-configurator": "file:../jest-configurator",
    "@times-components-native/jest-serializer": "file:../jest-serializer",
    "@times-components-native/provider": "file:../provider",
    "@times-components-native/provider-queries": "file:../provider-queries",
    "@times-components-native/provider-test-tools": "file:../provider-test-tools",
