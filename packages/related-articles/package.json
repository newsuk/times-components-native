{
  "name": "@times-components-native/related-articles",
  "private": true,
  "version": "6.5.7",
  "description": "related articles of an article",
  "main": "src/related-articles",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,react-art' --ignore-bin-package=false --skip-missing",
    "lint": "eslint src && yarn prettier:diff",
    "test": "jest roles.test.js --config='./__tests__/jest.config.js'",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "related-articles",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/storybook": "file:../storybook",
    "@times-components-native/tealium-utils": "file:../tealium-utils",
    "@times-components-native/test-utils": "file:../test-utils"
  },
  "dependencies": {
    "@times-components-native/article-summary": "file:../article-summary",
    "@times-components-native/card": "file:../card",
    "@times-components-native/link": "file:../link",
    "@times-components-native/markup-forest": "file:../markup-forest",
    "@times-components-native/slice-layout": "file:../slice-layout",
    "@times-components-native/styleguide": "file:../styleguide",
    "@times-components-native/tracking": "file:../tracking",
    "lodash.get": "4.4.2",
    "prop-types": "15.7.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
