{
  "name": "@times-components-native/styleguide",
  "private": true,
  "version": "3.38.10",
  "description": "Centralised library for common styles throughout the Times",
  "main": "src/styleguide",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier,react-art' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "styleguide",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/test-utils": "file:../test-utils"
  },
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
