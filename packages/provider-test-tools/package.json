{
  "name": "@times-components-native/provider-test-tools",
  "private": true,
  "version": "1.19.1",
  "description": "utillity for testing apollo-client's behaviour",
  "main": "src/index",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,**/eslint*,graphql,jest,prettier,jest-transform-graphql' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "testEnvironment": "jsdom",
    "coverageDirectory": "<rootDir>/packages/provider-test-tools/coverage/",
    "collectCoverageFrom": [
      "**/packages/provider-test-tools/src/*.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/provider-test-tools/__tests__/**.test.js"
    ],
    "testURL": "http://localhost",
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "graphql",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/test-utils": "file:../test-utils"
  },
  "dependencies": {
    "@times-components-native/fixture-generator": "file:../fixture-generator",
    "@times-components-native/provider-queries": "file:../provider-queries",
    "@times-components-native/schema": "file:../schema",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.4",
    "apollo-utilities": "1.2.1",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.3",
    "graphql-tools": "4.0.4",
    "lodash.isequal": "4.4.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-apollo": "2.5.5",
    "react-test-renderer": "16.9.0",
    "zen-observable": "0.8.11"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
