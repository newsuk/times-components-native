{
  "name": "@times-components-native/provider-test-tools",
  "private": true,
  "version": "1.19.1",
  "description": "utillity for testing apollo-client's behaviour",
  "main": "src/index",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,**/eslint*,graphql,jest,prettier,jest-transform-graphql' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "testEnvironment": "jsdom",
    "coverageDirectory": "<rootDir>/packages/provider-test-tools/coverage/",
    "collectCoverageFrom": [
      "**/packages/provider-test-tools/src/*.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/provider-test-tools/__tests__/**.test.js"
    ],
    "testURL": "http://localhost",
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "graphql",
    "provider",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@times-components-native/eslint-config-thetimes": "0.8.16",
    "@times-components-native/test-utils": "2.3.8",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "1.10.6",
    "depcheck": "0.6.9",
    "eslint": "5.9.0",
    "eslint-plugin-graphql": "2.1.1",
    "jest": "24.8.0",
    "jest-transform-graphql": "2.1.0",
    "prettier": "1.14.3"
  },
  "dependencies": {
    "@times-components-native/fixture-generator": "0.28.1",
    "@times-components-native/provider-queries": "2.14.1",
    "@times-components-native/schema": "0.6.23",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.4",
    "apollo-utilities": "1.2.1",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.3",
    "graphql-tools": "4.0.4",
    "lodash.isequal": "4.4.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-apollo": "2.5.5",
    "react-test-renderer": "16.9.0",
    "zen-observable": "0.8.11"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
