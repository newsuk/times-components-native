{
  "name": "@times-components-native/svgs",
  "private": true,
  "version": "2.7.30",
  "description": "Svgs wrapper",
  "main": "src/index",
  "scripts": {
    "fmt": "eslint . --fix && prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "depcheck": "depcheck --ignores='@babel/*,babel-*,depcheck,eslint,jest,prettier' --ignore-bin-package=false --skip-missing",
    "lint": "eslint . && yarn prettier:diff && yarn depcheck",
    "test:android": "jest --config='./__tests__/android/jest.config.js'",
    "test:ios": "jest --config='./__tests__/ios/jest.config.js'",
    "test:all": "yarn test:android --coverage && yarn test:ios --coverage"
  },
  "jest": {
    "preset": "react-native",
    "rootDir": "../../",
    "coverageDirectory": "<rootDir>/packages/svgs/coverage/",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-svg)/)"
    ],
    "testMatch": [
      "<rootDir>/packages/svgs/__tests__/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components-native.git"
  },
  "keywords": [
    "react",
    "native",
    "web",
    "component"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@times-components-native/test-utils": "file:../test-utils"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react-native-svg": "9.11.1"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "peerDependencies": {
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-native": ">=0.59"
  },
  "publishConfig": {
    "access": "public"
  }
}
